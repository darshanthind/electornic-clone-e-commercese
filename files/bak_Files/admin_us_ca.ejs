<%-include("admin_header",a)%>
<div id="page-wrapper" style="position: relative;top:30px;">
    <div id="page-inner">
        <div class="row">
            <div class="col-md-12">
             <h2>User Select Item Carts</h2>   
                <h5>Table </h5>
               
            </div>
        </div>
         <!-- /. ROW  -->
         <hr />
       
    <div class="row">
        <div class="col-md-12">
            <!-- Advanced Tables -->
            <div class="panel panel-default">
                <div class="panel-heading">
                     User Carts
                </div>
                <div class="panel-body">
                    <div class="table-responsive">
                        <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                            <thead>
                                <tr>
                                    <th>Name</th>
                                    <th>id</th>
                                    <th>Product Quentity</th>
                                    
                                    <th>View</th>
                                    <th>Remove</th>
                                    
                                </tr>
                            </thead>
                            <tbody>
                                <% for(let p of d){%>
                                    <tr>
                                    <td><%=p.Username%></td>
                                    <td><%=p.id%></td>
                                    <td><%=p.Products.length%></td>
                                    <td><a href="/admin/user_ca_p/<%=p._id%>">view</a></td>
                                    <td>Remove</td>
                                    </tr>
                                <%}%>
                              </tbody>
                        </table>
                    </div>
                    
                </div>
            </div>
            <!--End Advanced Tables -->
        </div>
    </div>